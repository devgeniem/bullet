{
  "name": "bullet",
  "version": "0.0.1",
  "description": "A project for creating a RESTful web api boilerplate with sane architecture using Node.js, TypeScript and Express.js",
  "main": "./src/server.ts",
  "scripts": {
    "test": "echo \"Tests have not been setup yet!\" && exit 0",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "compile": "tsc",
    "prerun": "npm run compile",
    "run": "node ./build/server.js",
    "start": "tsc-watch --onSuccess \"node ./build/server.js\" --onFailure \"echo Compilation failed! \""
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "node",
    "express",
    "typescript",
    "web-api"
  ],
  "contributors": [
    {
      "name": "Topi Latva-Salo",
      "email": "topi.latva-salo@geniem.com"
    },
    {
      "name": "Timo Laakso",
      "email": "timo.laakso@geniem.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "apisauce": "^1.0.0",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "knex": "^0.15.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "objection": "^1.4.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.16.0",
    "@types/inversify": "^2.0.33",
    "@types/knex": "^0.15.1",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.10",
    "inversify-logger-middleware": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "ts-node": "^8.4.1",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  }
}
